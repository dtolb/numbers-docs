!fileScope internal

description: Retrieves the total count of Toll-Free Telephone Numbers for the given site.

get:
    description: Retrieves the total count of Toll-Free Telephone Numbers for the given site.
    responses:
        200:
            body:
                application/xml:
                    example: |
                        <SipPeerTollFreeTotalsResponse>
                            <TollFreeTotals>
                                <TotalCount>0</TotalCount>
                            </TollFreeTotals>
                        </SipPeerTollFreeTotalsResponse>
